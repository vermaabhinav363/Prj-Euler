#include <bits/stdc++.h>
#define ll long long
#define pb push_back
using namespace std;
#define INF 1e18
const ll MOD = 1e9+7;


void SieveOfEratosthenes(ll n)
{
    bool prime[n + 1];

    memset(prime, true, sizeof(prime));
 
    for (ll p = 2; p * p <= n; p++) {

        if (prime[p] == true) {
            for (ll i = p * p; i <= n; i += p)
                prime[i] = false;
        }
    }
    ll m=-1;
    for(ll i =1;i<sqrt(600851475143);i++){
        if(600851475143 % i == 0){
            if(i<n + 1 && prime[i])
            m = max(m,i);
            if(600851475143/i < n+1 && prime[600851475143/i])
            m = max(m,600851475143/i);

        }
    }
    cout<<m;
    
}

int main()
 
{
 
#ifndef ONLINE_JUDGE
 
    freopen("input.txt", "r", stdin);
 
    freopen("output.txt", "w", stdout);
   
#endif
    
    ll n = sqrt(600851475143);
    SieveOfEratosthenes(n);
    return 0;


    
}
