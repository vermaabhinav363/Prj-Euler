#include <bits/stdc++.h>
#define ll long long int
#define pb push_back
using namespace std;
#define MOD 1000000007

bool prime[1000001];
void SieveOfEratosthenes(int n)
{
    memset(prime, true, sizeof(prime));
 
    for (int p = 2; p * p <= n; p++)
    {
 
        if (prime[p] == true)
        {
 
            for (int i = p * p; i <= n; i += p)
                prime[i] = false;
        }
    }
}

 
int main()
 
{
 
#ifndef ONLINE_JUDGE
 
    freopen("input.txt", "r", stdin);
 
    freopen("output.txt", "w", stdout);
   
#endif
   ll m=-1;
   string str;
   cin>>str;
   for(int i =0;i<1000;i++){
        ll pro=1;
        ll count=0;
        for(int j=0;j<13;j++){
            if(i+j<1000){
                pro*= (str[i+j]-'0');
                count++;
            }
        }
        if(count==13)
        m=max(m,pro);
   }
   cout<<m;

}
